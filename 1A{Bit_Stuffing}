#include<stdio.h>
int main(){
    int a[50],b[100],n,j=0,count=0;
    scanf("%d",&n);
    for(int i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    for(int i=0;i<n;i++) {
        b[j++]=a[i];
        if(a[i]==1){
            count++;
            if(count==5){
                b[j++]=0;
                count=0;
            }
        }
        else{
            count=0;
        }
    }
    for(int i=0;i<j;i++) {
        printf("%d", b[i]);
    }
}
